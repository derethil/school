/filter {
    2 dict begin
    count 0 ne {
        /func exch def
        count % repeat for every item on stack
        {
            count -1 roll % move bottom number on stack to the top
            /toTest exch def
            toTest /func load exec not % only leave number on stack if func evaluates to false
                { toTest }
            if
        } repeat
    }
    if
    end
} def
